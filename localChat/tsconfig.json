{
  "compilerOptions": {
    "module": "CommonJS",
    "baseUrl": "./",
    "moduleResolution": "node",
    "lib": ["es2016", "dom", "es2022"],
    "types": ["vite/client"],
    "paths": {
      "@cpt/*": ["views/components/*"],
      "@js/*": ["views/js/*"],
      "@common/*":[ "common/*"],
      "@pinia/*": ["views/pinia/*"],
      "@style/*": ["views/assets/style/*"],
      "@image/*": ["views/assets/image/*"],
    }
  }
}